{
  "_version": "1.40.0",
  "sap.app": {
    "id": "nucleus.mainConfig",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap-ux/fiori-freestyle-writer:basic",
      "version": "0.12.1"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZNU_CONFIG_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/ZNU_CONFIG_SRV.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.102.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "nucleus.mainConfig.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true
      }
    },
    "resources": {
      "css": [{
        "uri": "css/style.css"
      }],
      "js": [{
        "uri": "libs/xlsx.full.min.js",
        "uri2": "libs/filesaver.js"
      }]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "nucleus.mainConfig.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [{
          "name": "RouteMainView",
          "pattern": ":?query:",
          "target": [
            "TargetMainView"
          ]
        },
        {
          "name": "midOptionView",
          "pattern": "midOption",
          "target": [
            "MidOptionView"
          ]
        },
        {
          "name": "plantCreateView",
          "pattern": "plantCreate",
          "target": [
            "PlantCreateView"
          ]
        },
        {
          "name": "plantDownloadView",
          "pattern": "plantDownload",
          "target": [
            "PlantDownloadView"
          ]
        },
        {
          "name": "plantMassView",
          "pattern": "plantMass",
          "target": [
            "PlantMassView"
          ]
        }
      ],
      "targets": {
        "TargetMainView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MainView",
          "viewName": "MainView"
        },
        "MidOptionView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MidOptionView",
          "viewName": "MidOption"
        },
        "PlantCreateView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "plantCreateView",
          "viewName": "PlantCreate"
        },
        "PlantDownloadView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "plantDownloadView",
          "viewName": "PlantDownload"
        },
        "PlantMassView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "plantMassView",
          "viewName": "PlantMass"
        }
      }
    },
    "rootView": {
      "viewName": "nucleus.mainConfig.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}